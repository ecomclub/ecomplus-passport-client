  "author": "E-Com Club",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomclub/ecomplus-passport-client/issues"
  },
  "homepage": "https://passport.e-com.plus",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@ecomplus/client": "^1.0.3",
    "@ecomplus/utils": "^1.0.1",
    "core-js": "^3.1.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-loader": "^8.0.6",
    "docdash": "^1.1.1",
    "husky": "^3.0.4",
    "jsdoc": "^3.6.3",
    "standard": "^13.1.0",
    "standard-version": "^6.0.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
