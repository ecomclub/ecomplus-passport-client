{
  "name": "ecomplus-passport-client",
  "version": "0.0.1",
  "description": "JS client script to use E-Com Plus Passport on storefront",
  "main": "dist/ecomplus-passport-client.min.js",
  "module": "src/index.js",
  "browser": "dist/ecomplus-passport-client.min.js",
  "jsdelivr": "dist/ecomplus-passport-client.root.min.js",
  "unpkg": "dist/ecomplus-passport-client.root.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
